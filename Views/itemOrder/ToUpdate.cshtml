<ul class="product-list">
    <form asp-action="ToUpdate" method="post">
        <li class="product-item a it1">
            <article class="article-ne">
                <a target="_blank" class="product-link">
                    <figure class="product-info">
                        <div class="product-info-img box1">
                            <img class="productA" src="~/CSS/img/mug.jpg" alt="description image">
                        </div>
                        <figcaption class="product-info-description">

                            <p class="description i"> <input class="prod prod1" asp-for="description" type="text"
                                    readonly value="Caneca halloween" required></p>
                            <ul class="star-ranking">
                                <li><img src="https://i.ibb.co/2kW5mnM/star.png">
                                </li>
                                <li><img src="https://i.ibb.co/2kW5mnM/star.png">
                                </li>
                                <li><img src="https://i.ibb.co/2kW5mnM/star.png">
