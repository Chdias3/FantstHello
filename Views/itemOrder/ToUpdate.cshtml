                                </li>
                                <li><img src="https://i.ibb.co/TwYYb93/star-1.png">
                                </li>
                                <li class="qtd-votes">
                                    </span>(200)</span>
                                </li>
                            </ul>
                        </figcaption>
                        <div class="price-box box2">
                            <div class="price">
                                <div>
                                    <input readonly value="100,00" class="prod pd1" asp-for="value_unitary" />
                                    <del>R$200,00</del>
                                    <ins class="d-block">R$100,00</ins>
                                </div>
                                <span class="price-info">4x de R$ 25,00</span>
                            </div>
                            <button type="submit" class="btn btn-buy">Alterar <i
                                    class="fas fa-arrow-right"></i></button>
                            <p class="inpq">
                                <input class="quant" asp-for="quantity" required />
                            </p>

                            <p> <input asp-for="productPhoto" type="hidden" readonly
                                    value="http://localhost:5000/CSS/img/mug.jpg" required></p>

                            <p> <input asp-for="idProduct" type="hidden"></p>

                            <p> <input asp-for="idUser" value=@Context.Session.GetInt32("IdUsuario") type="hidden"
                                    readonly></p>

                        </div>
                        </div>

                    </figure>

                </a>
            </article>

        </li>
    </form>

